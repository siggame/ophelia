{
  "name": "webserver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "start": "node ./server/bin/www",
    "debug": "concurrently \"npm run server-debug\" \"cd client && npm start\"",
    "server-debug": "cd server && nodemon ./bin/www",
    "setup": "npm install && cd client && npm install && cd ../server && npm install && cd ..",
    "testing": "npm install nightwatch && npm install selenium-standalone && ./node_modules/.bin/selenium-standalone install"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "concurrently": "^3.5.0",
    "eslint": "^4.7.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-flowtype": "^2.43.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "nightwatch": "^0.9.19",
    "react-paginate": "^5.1.0",
    "selenium-standalone": "^6.12.0"
  }
}
